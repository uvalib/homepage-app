{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build/unbundled",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/",
        "headers": [{"key": "Link", "value": "</src/uvalib-app.html>;rel=preload,</bower_components/polymer/polymer.html>;rel=preload,</bower_components/polymer/polymer-mini.html>;rel=preload,</bower_components/polymer/polymer-micro.html>;rel=preload,</js/service-worker.js>;rel=preload;as=script"}]
      }
    ]
  }
}
